<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/hight_light_text</item>
        <item name="colorPrimaryDark">@color/hight_light_text</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:fitsSystemWindows">true</item>
    </style>
    <style name="Loginedittext" >
        <item name="android:textColor">@color/transparent_black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="Loginedittextuser" >
        <item name="android:textColor">@color/transparent_black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingLeft">@dimen/x140</item>
    </style>
    <style name="color_dialog" parent="android:Theme.Dialog">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.4</item>
    </style>
</resources>
